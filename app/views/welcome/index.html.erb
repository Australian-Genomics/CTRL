<body class="bg-white">
<header id="header" class="header">
  <div class="container d-flex">
    <a href="/" class="header__logo d-block mt-45 mx-auto ml-md-0">
      <%= image_tag "logo@3x.png", alt: "logo", class: "h-100" %>
    </a>
  </div>
</header>
<div class="landing-page">
  <div class="container mt-120">
    <div class="row">
      <div class="col-12 col-md-11 col-lg-8 col-xl-8 mt-5 mb-5">
        <p class="landing-page__text mt-45 text-dark-purple">
          <h2>Consent to participating in the Australian Genomics program</h2>
        </p>
        <div class="mt-45">
          <%= link_to "Register Now", new_registration_path(User.new), class: "btn btn-success mr-4" %>
          <%= link_to "Log in", new_session_path(User), class: "btn btn-outline-success" %>
        </div>
      </div>
    </div>
  </div>
  <div class="landing-page__bg"></div>
</div>
</body>